{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "license": "AGPL",
  "dependencies": {
    "@tippy.js/react": "^2.2.2",
    "country-region-data": "^1.4.5",
    "express": "4.16.4",
    "final-form": "^4.16.1",
    "final-form-submit-listener": "^1.0.0",
    "firebase": "^6.3.0",
    "humanize-string": "^2.1.0",
    "localforage": "^1.7.3",
    "localforage-observable": "^2.0.0",
    "navi": "^0.13.0-alpha.0",
    "npm-run-all": "^4.1.5",
    "polished": "^3.4.1",
    "popup-trigger": "0.2.2",
    "raw-loader": "^3.0.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-final-form": "^6.3.0",
    "react-navi": "^0.13.0-beta.4",
    "react-navi-helmet-async": "^0.13.0-beta.4",
    "react-perfect-scrollbar": "^1.5.3",
    "react-popper": "^1.3.3",
    "react-spring": "^8.0.27",
    "react-stripe-elements": "^4.0.0",
    "rimraf": "^2.6.3",
    "styled-components": "^4.3.2",
    "universal-react-scripts": "3.0.3",
    "zen-observable": "^0.8.14"
  },
  "main": "build/node/index.js",
  "files": [
    "build/node/*.*"
  ],
  "scripts": {
    "start:api": "firebase serve --only functions:api",
    "start:app": "react-scripts start",
    "start": "npm-run-all --parallel start:*",
    "build": "react-scripts build",
    "deploy:api": "rimraf functions/renderer.tgz && firebase deploy --only functions:api",
    "deploy:app": "npm run deploy -- --only hosting,functions:renderer",
    "deploy": "firebase deploy",
    "predeploy": "sh ./scripts/pack.sh",
    "serve": "react-scripts serve",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "no-restricted-imports": [
        "error",
        {
          "paths": [
            {
              "name": "styled-components",
              "message": "Please import from styled-components/macro."
            }
          ],
          "patterns": [
            "!styled-components/macro"
          ]
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
